{
  "name": "mfml-example",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run mfmlc && webpack --mode=production",
    "watch": "npm run clean && npm run mfmlc && webpack serve --mode=development",
    "mfmlc": "mfmlc --include './src/translations/*.json' --outDir ./src/gen --ts",
    "clean": "rimraf ./src/gen ./target"
  },
  "devDependencies": {
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "html-webpack-plugin": "^5.3.2",
    "lodash": "^4.17.21",
    "mfml-cli": "^1.0.0",
    "mfml-compiler": "^1.0.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "dayjs": "^1.10.6",
    "mfml-react-runtime": "^1.0.0",
    "mfml-runtime": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
