{
  "name": "example",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run mfmlc && rollup --config --environment NODE_ENV:production",
    "watch": "npm run clean && npm run mfmlc && rollup --config --environment NODE_ENV:development --watch",
    "mfmlc": "mfmlc --include './src/translations/*.json' --outDir ./src/gen --ts",
    "clean": "rimraf ./src/gen ./target"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "mfml-cli": "^1.0.0",
    "mfml-compiler": "^1.0.0",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "dayjs": "^1.10.6",
    "mfml-runtime": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
