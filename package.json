{
  "name": "icuc",
  "version": "1.0.0",
  "description": "The ICU + XML/HTML compiler and runtime.",
  "main": "./lib/index-cjs.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "set -e; tsc; npm run rollup; npm run rollup-bin;",
    "rollup": "rollup --input ./lib/index.js --file ./lib/index-cjs.js --format cjs --plugin @rollup/plugin-node-resolve --plugin @rollup/plugin-commonjs",
    "rollup-bin": "rollup --external commander,glob,@messageformat/parser,@smikhalevski/codegen,tag-soup --exports named --input ./lib/mfml-bin.js --file ./lib/mfml-bin.js --format cjs --banner '#!/usr/bin/env node' --plugin @rollup/plugin-node-resolve",
    "rollup-runtime": "rollup --exports named --input ./lib/jtd-dialect/runtime.js --file ./lib/jtd-dialect/runtime.js --format cjs --plugin @rollup/plugin-node-resolve",
    "terser": "terser --compress --mangle toplevel --output ./lib/index-cjs.js -- ./lib/index-cjs.js",
    "clean": "rm -rf ./lib",
    "test": "jest --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/icuc.git"
  },
  "keywords": [
    "i18n",
    "html",
    "language",
    "parser",
    "translation",
    "compiler",
    "xml",
    "icu",
    "l10n",
    "intl",
    "tms"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/icuc/issues"
  },
  "homepage": "https://github.com/smikhalevski/icuc#readme",
  "jest": {
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/glob": "^7.1.4",
    "@types/jest": "^26.0.23",
    "@types/moo": "^0.5.4",
    "@types/react": "^17.0.11",
    "jest": "^27.0.2",
    "rollup": "^2.50.5",
    "terser": "^5.7.0",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@messageformat/parser": "^5.0.0",
    "@smikhalevski/codegen": "^1.0.0",
    "commander": "^8.0.0",
    "glob": "^7.1.7"
  }
}
